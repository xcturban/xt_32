<script language="javascript">
var obj_width=10;
var box_width=150;

var points=17;
var division=box_width/(points);

function on_submit(){
   document.forms[0].talk_volume_in.value=finger_get_value($("key_1"));
   document.forms[0].talk_volume_out.value=finger_get_value($("key_2"));
   document.forms[0].broadcast_volume_in.value=finger_get_value($("key_3"));
   document.forms[0].broadcast_volume_out.value=finger_get_value($("key_4"));
   document.forms[0].terminal_volume_ring.value=finger_get_value($("key_5"));

	on_select_change_dynamic(0);
	return true;
}

$("button_1").onmousedown=function(){
  finger_bind_button(this, document.body, $("key_1"));
}
$("button_2").onmousedown=function(){
  finger_bind_button(this, document.body, $("key_2"));
}
$("button_3").onmousedown=function(){
  finger_bind_button(this, document.body, $("key_3"));
}
$("button_4").onmousedown=function(){
  finger_bind_button(this, document.body, $("key_4"));
}
$("button_5").onmousedown=function(){
  finger_bind_button(this, document.body, $("key_5"));
}


finger_set_value($("button_1"), $("key_1"), document.forms[0].talk_volume_in.value, division);
finger_set_value($("button_2"), $("key_2"), document.forms[0].talk_volume_out.value, division);
finger_set_value($("button_3"), $("key_3"), document.forms[0].broadcast_volume_in.value, division);
finger_set_value($("button_4"), $("key_4"), document.forms[0].broadcast_volume_out.value, division);
finger_set_value($("button_5"), $("key_5"), document.forms[0].terminal_volume_ring.value, division);

put_radio("talk_sample_rate",<?cgi talk.sample_rate ?>0/10 );
put_radio("talk_encode_mode",<?cgi talk.encode_mode ?>0/10);
put_radio("talk_echo",<?cgi talk.echo ?>0/10);
put_radio("broadcast_sample_rate",<?cgi broadcast.sample_rate ?>0/10);
put_radio("broadcast_encode_mode",<?cgi broadcast.encode_mode ?>0/10);
put_radio("broadcast_alert_audio",<?cgi broadcast.alert_audio ?>0/10);
put_radio("terminal_ring_channel",<?cgi terminal.ring_channel ?>0/10);


$("id_obj_ret").value+="?time="+(new Date()).getTime();
$("id_obj_ret").value=urlEncode($("id_obj_ret").value);
</script>

</HTML>
