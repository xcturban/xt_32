<script>
function on_submit(){
	var ret=true;
	if($("id_password_1").value!=$("id_password_2").value){
		alert("Error. Password inkey twice not the same!");
		return false;
	}
}
$("id_obj_ret").value=urlEncode($("id_obj_ret").value);
</script>
